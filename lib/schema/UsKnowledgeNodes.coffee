#console.log 'Loading UsKnowledgeNodes.coffee'

## 本数据库涉及到的几个Colleciotn首先列出
##  COLLECTION
@Laniakea.Collection.UsKnowledgeNodes = new Mongo.Collection ("usKnowledgeNodes")

##  INDEX
if Meteor.isServer
  @Laniakea.Collection.UsKnowledgeNodes._ensureIndex {si: 1}

##  SCHEMA

@Laniakea.Schema.UsKnowledgeNode = new SimpleSchema
  name:
    type: String
    label: '名称'
    optional: false

  cat1:
    type: String
    label: '检查部位'
    optional: true

  cat2:
    type: String
    label: '检查项目'
    optional: true
  cat3:
    type: String
    label: '三级目录'
    optional: true

  find:
    type: String
    label: "超声所见"
    optional: true

  hint:
    type: String,
    label: '超声所得'
    optional: true

  si:
    type: String
    optional: true
    autoform:
      afFieldInput:
        type: 'hidden'
    autoValue: ->
      si(this,['name','cat1','cat2','cat3'])

Laniakea.Collection.UsKnowledgeNodes.attachSchema Laniakea.Schema.UsKnowledgeNode


##PUBLISH
if Meteor.isServer
  Meteor.publish 'usKnowledgeNodes', (selector, options)->
    if selector?.si
      selector.si = new RegExp(selector.si, 'i')
    options['fields'] =
      name: 1
      cat1: 1
      cat2: 1
      cat3: 1
      si: 1
    Laniakea.Collection.UsKnowledgeNodes.find selector, options

  Meteor.publish 'singleUsKnowledgeNode', (id)->
    Laniakea.Collection.UsKnowledgeNodes.find id

#PERIMISSION
Laniakea.Collection.UsKnowledgeNodes.allow
  insert: ->
    true
  remove: ->
    true
  update: ->
    true

